@{
    ViewData["Title"] = "Index";
}
<h1 class="text-center">Individual User</h1>
<section class="section">

    <div class="container border rounded shadow p-4 bg-light">
        <table id="tblIndividuals" class="table table-striped table-bordered table-hover text-center">
            <thead class="table-dark">
                <tr>
                    <th>Profile</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone Number</th>
                    <th>Role</th>
                    <th>Action</th>
                </tr>
            </thead>
        </table>
    </div>
</section>

@section Scripts {
    <script src="~/js/user.js"></script>
}
